package exercicio04;
import java.util.Scanner;

public class Exercicio04 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int mat[][] = new int[5][5];
		boolean aproção = false;
		
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 5; j++) {
				System.out.print("Digite um número: ");
				mat[i][j]= sc.nextInt();
				
				
			}
		}
	}
}
