package exercicio01;
import java.util.Scanner;

public class Exercicio01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int maior = 0;

		for(int n = 0; n < 10; n++) {
			System.out.print("Digite um nÃºmero: ");
			int num = sc.nextInt();
			
			if(n == 0) {
				maior = num;
			}
		}
	}
}
