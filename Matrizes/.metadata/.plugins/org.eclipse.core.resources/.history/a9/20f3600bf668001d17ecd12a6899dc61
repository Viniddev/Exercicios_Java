package exercicio02;
import java.util.Scanner;


public class Exercicio02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int matriz[][] = new int[5][5];

		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 5; j++) {
				if(i == j) {
					matriz[i][j] = 1;
				}else {
					matriz[i][j] = 0; 
				}
				System.out.print(matriz[i][j] + "  ");
			}
			System.out.println("");
		}
	}
}
